
// Step 1: Specify Region of Interest
// Option 1: Use the "Geometry tools" on the map to draw your region of interest (ROI)

// Option 2: Specify latitude and longitude boundaries (uncomment lines)
var minLat = 30;
var minLon = 5;
var maxLat = 30.3;
var maxLon = 5.1;
var geometry = ee.Geometry.Rectangle([minLon, minLat, maxLon, maxLat]);

// Step 2: Specify your date range:
var startDate = '2024-01-01';
var endDate = '2025-01-01';

// Step 3: Load Dynamic World collection
var dw = ee.ImageCollection('GOOGLE/DYNAMICWORLD/V1')
  .filter(ee.Filter.date(startDate, endDate)) // specify your date range
  .filter(ee.Filter.bounds(geometry))

// Step 4: Create a Mode Composite
var classification = dw.select('label')
var dwComposite = classification.reduce(ee.Reducer.mode());

var dwVisParams = {
  min: 0,
  max: 8,
  palette: ['#419BDF', '#397D49', '#88B053', '#7A87C6',
    '#E49635', '#DFC35A', '#C4281B', '#A59B8F', '#B39FE1']
};

// Step 5: Clip the composite and add it to the Map
Map.centerObject(geometry)
Map.addLayer(dwComposite.clip(geometry), dwVisParams, 'Classified Composite') 

// Step 6: Export the landcover as a GeoTiff
Export.image.toDrive({
  image: dwComposite.clip(geometry),
  description: 'DynamicWorldExport',
  region: geometry,
  fileFormat: 'GeoTIFF',
  scale: 10, // lowest resolution is 10 meters
  maxPixels: 1e10})


  